<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".MyPage.MyPageProfileFragment">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_mypage_profile_nickname_title"
        style="@style/Medium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginTop="60dp"
        android:text="닉네임"
        android:textColor="@color/disabled"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <View
        android:id="@+id/divider7"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="22dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tv_mypage_profile_nickname"
        app:layout_constraintTop_toBottomOf="@+id/tv_mypage_profile_nickname" />

    <TextView
        android:id="@+id/tv_mypage_profile_nickname"
        style="@style/XLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:text="고독한 사자"
        android:textColor="@color/text"
        app:layout_constraintStart_toStartOf="@+id/tv_mypage_profile_nickname_title"
        app:layout_constraintTop_toBottomOf="@+id/tv_mypage_profile_nickname_title" />

    <TextView
        android:id="@+id/tv_mypage_profile_email_title"
        style="@style/Medium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="이메일"
        android:textColor="@color/disabled"
        app:layout_constraintStart_toStartOf="@+id/divider7"
        app:layout_constraintTop_toBottomOf="@+id/divider7" />

    <TextView
        android:id="@+id/tv_mypage_profile_email"
        style="@style/XLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:text="Email@gmail.com"
        android:textColor="@color/text"
        app:layout_constraintStart_toStartOf="@+id/tv_mypage_profile_email_title"
        app:layout_constraintTop_toBottomOf="@+id/tv_mypage_profile_email_title" />

    <View
        android:id="@+id/divider8"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="22dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tv_mypage_profile_email"
        app:layout_constraintTop_toBottomOf="@+id/tv_mypage_profile_email" />

</androidx.constraintlayout.widget.ConstraintLayout>
